{
    "MOUNTS": {
        "ROOT": {
            "selector": ".mount__list__item",
            "multiple": true
        },
        "NAME": {
            "selector": ".mount__name",
            "type": "string"
        },
        "ICON": {
            "selector": ".mount__list__icon__image",
            "attribute": "src",
            "type": "string"
        }
    },
    "TOTAL": {
        "selector": ".minion__sort__total > span:nth-child(1)",
        "type": "integer"
    }
}
