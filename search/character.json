{
    "ENTRIES": {
        "selector": ".ldst__window > div.entry"
    },
    "ENTRY": {
        "AVATAR": {
            "selector": ".entry__chara__face > img"
        },
        "ID": {
            "selector": ".entry__link",
            "regex": "/lodestone/character/(?P<ID>\\d*)/"
        },
        "LANG": {
            "selector": ".entry__chara__lang"
        },
        "NAME": {
            "selector": ".entry__name"
        },
        "RANK": {
            "selector": ".entry__chara_info > .js__tooltip"
        },
        "RANK_ICON": {
            "selector": ".entry__chara_info > .js__tooltip > img"
        },
        "SERVER": {
            "selector": ".entry__world"
        }
    },
    "LIST_NEXT_BUTTON": {
        "selector": "ul.btn__pager:nth-child(7) > li:nth-child(4) > a:nth-child(1)"
    },
    "PAGE_INFO": {
        "selector": "#community > div.ldst__bg > div.ldst__contents.clearfix > div.ldst__main > div > ul:nth-child(57) > li.btn__pager__current"
    }
}